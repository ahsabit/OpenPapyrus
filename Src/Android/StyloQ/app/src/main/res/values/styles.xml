<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <style name="FakeView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="ToolbarButton">
        <item name="android:layout_width">@dimen/toolbariconsqside</item>
        <item name="android:layout_height">@dimen/toolbariconsqside</item>
        <item name="android:onClick">OnButtonClk</item>
        <item name="android:background">@drawable/selector_imagebutton</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginRight">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
    </style>
    <style name="ListItemButton">
        <item name="android:layout_width">@dimen/listviewiconsqside</item>
        <item name="android:layout_height">@dimen/listviewiconsqside</item>
        <item name="android:onClick">OnButtonClk</item>
        <item name="android:background">@drawable/selector_imagebutton</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginRight">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
    </style>
    <!-- parent="Widget.MaterialComponents.Button.OutlinedButton" -->
    <style name="PushButton">
        <item name="android:onClick">OnButtonClk</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@drawable/selector_button</item>
        <item name="android:strokeColor">@color/Divider</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@color/TextPrimary</item>
        <item name="android:paddingLeft">6dp</item>
        <item name="android:paddingRight">6dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:minHeight">32dp</item>
        <item name="android:minWidth">80dp</item>
        <item name="android:layout_marginLeft">4dp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:layout_marginBottom">4dp</item>
    </style>
    <style name="ImagePushButton" parent="PushButton">
        <item name="android:background">@drawable/selector_imagebutton</item>
    </style>
    <!-- parent="@style/Widget.AppCompat.Spinner.Underlined" -->
    <style name="ComboBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">160dp</item>
        <item name="android:minHeight">40dp</item>
        <item name="android:dropDownWidth">match_parent</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:background">@drawable/shape_combobox</item>
        <item name="android:popupBackground">@drawable/shape_button</item>
        <item name="android:spinnerMode">dropdown</item>
    </style>
    <style name="InputLine" parent="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <!--
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        -->
        <item name="android:layout_margin">2dp</item>
        <item name="boxStrokeColor">@color/PrimaryDark</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">13sp</item>
    </style>
    <style name="TabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">12sp</item>
        <!-- <item name="android:fontWeight">600</item> -->
        <item name="android:textColor">@color/TextPrimary</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
    <style name="TabLayout" parent="Widget.Design.TabLayout">
        <!--<item name="tabMaxWidth">40dp</item>-->
        <item name="android:layout_height">wrap_content</item>
        <item name="tabIndicatorColor">?attr/colorAccent</item>
        <item name="tabIndicatorHeight">6dp</item>
        <!--<item name="tabBackground">?attr/selectableItemBackground</item>-->
        <!--<item name="tabBackground">@drawable/shape_tab</item>-->
        <item name="tabTextAppearance">@style/TabTextAppearance</item>
        <item name="tabSelectedTextColor">@color/TextSecondary</item>
        <item name="tabPadding">2dp</item>
        <item name="tabMode">scrollable</item>
        <item name="tabGravity">fill</item>
        <item name="tabInlineLabel">true</item>
    </style>
    <style name="DialogFrameRegular">
        <item name="android:background">@drawable/shape_viewframe</item>
        <item name="android:colorBackground">@color/ListItemRegular</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">1dp</item>
    </style>
    <style name="ListItemRegular">
        <item name="android:background">@drawable/shape_listitem</item>
        <item name="android:colorBackground">@color/ListItemRegular</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">1dp</item>
    </style>
    <style name="ListItemWithTitle">
        <item name="android:background">@drawable/shape_listitem_withtitle</item>
        <item name="android:colorBackground">@color/ListItemRegular</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">1dp</item>
    </style>
    <style name="PageHeaderRegular">
        <item name="android:background">@drawable/shape_pageheader</item>
        <item name="android:padding">2dp</item>
        <item name="android:minHeight">48dp</item>
    </style>
    <style name="PageHeaderTitleText">
        <!-- <item name="android:fontFamily">@font/bagnard_sans</item> -->
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/TextPrimary</item>
        <item name="android:layout_margin">4dp</item>
    </style>
    <style name="PageHeaderTopicText">
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/TextPrimaryLight</item>
        <item name="android:layout_margin">4dp</item>
    </style>
    <style name="PageHeaderSvcTimeText">
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textSize">8sp</item>
        <item name="android:textColor">@color/TextPrimaryLight</item>
        <item name="android:layout_margin">4dp</item>
    </style>
    <style name="TopicText">
        <item name="android:textSize">12sp</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:textColor">@color/TextPrimaryLight</item>
    </style>
    <style name="TopicText2"> <!-- Наименование сервиса в элементе списка -->
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginRight">1dp</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:background">@drawable/shape_svcname</item>

        <item name="android:textSize">10sp</item>
        <!-- <item name="android:textFontWeight">600</item> -->
        <item name="android:padding">2dp</item>
        <item name="android:textAlignment">viewStart</item>
        <item name="android:textColor">@color/White</item>
    </style>
    <style name="PageBodyRegular">
        <item name="android:background">@drawable/shape_viewframe</item>
    </style>
    <style name="AttendanceMinutCell">
        <item name="android:background">@drawable/shape_viewframe</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginRight">1dp</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/TextPrimary</item>
        <item name="android:textSize">10sp</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
    </style>
    <style name="AttendanceMinutBlock">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_margin">2dp</item>
    </style>
    <style name="FormLabelText">
        <item name="android:layout_margin">2dp</item>
        <item name="android:padding">2dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <!--<item name="android:textFontWeight">500</item>-->
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/TextPrimaryLight</item>
    </style>
    <style name="HintText">
        <item name="android:layout_margin">4dp</item>
        <item name="android:padding">2dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/TextPrimary</item>
    </style>
    <style name="FormText">
        <item name="android:layout_margin">2dp</item>
        <item name="android:padding">2dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/TextPrimary</item>
    </style>
    <style name="TSessDurationText">
        <item name="android:textSize">10sp</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:textColor">@color/TextPrimary</item>
        <item name="android:layout_gravity">center_horizontal|center_vertical</item>
    </style>
    <style name="TSessPriceText">
        <item name="android:textSize">10sp</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:textColor">@color/Accent</item>
        <item name="android:layout_gravity">center_horizontal|center_vertical</item>
    </style>
    <style name="OrderedQtyText">
        <item name="android:textSize">10sp</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:textColor">@color/PrimaryDark</item>
        <item name="android:layout_gravity">center_horizontal|center_vertical</item>
    </style>
    <style name="ReportListItemText">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@color/TextPrimary</item>
        <item name="android:layout_marginLeft">4dp</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_marginBottom">4dp</item>
        <item name="android:layout_gravity">center_horizontal|center_vertical</item>
    </style>
    <style name="OrderListItemText">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/TextPrimary</item>
        <item name="android:layout_marginLeft">4dp</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_marginBottom">6dp</item>
        <item name="android:layout_gravity">center_horizontal|center_vertical</item>
    </style>
    <style name="GenericListItemText">
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_marginBottom">6dp</item>
        <item name="android:layout_marginLeft">4dp</item>
        <item name="android:textSize">13sp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@color/TextPrimary</item>
    </style>
    <style name="OuterImage_Side">
        <item name="android:padding">2dp</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <!-- <item name="android:background">@drawable/shape_frame_outer_image</item> -->
    </style>
    <style name="TextWithLabel">
        <item name="android:background">@drawable/shape_text_with_label_frame</item>
        <item name="android:layout_margin">2dp</item>
    </style>
    <style name="ExecuteTimeText">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:fontFamily">@font/liquidcrystal_normal</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/Black</item>
    </style>
    <style name="StatusIndicator">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">16dp</item>
        <item name="android:layout_gravity">center_vertical|right</item>
    </style>
    <style name="InfoFieldLabel">
        <item name="android:textColor">@color/TextPrimaryLight</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_marginLeft">12dp</item>
    </style>
    <style name="InfoFieldText">
        <item name="android:textColor">@color/TextPrimary</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginLeft">8dp</item>
    </style>
    <style name="InfoFieldGroup">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_marginBottom">4dp</item>
        <item name="android:background">@drawable/shape_listitem</item>
    </style>
</resources>
